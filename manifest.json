{
  "manifest_version": 3,
  "name": "Tab organizer",
  "version": "0.1.0",
  "description": "Manage your browsing sessions in a side panel.",
  "permissions": [
    "tabs",
    "sessions",
    "storage",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Open Tab Organizer"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  }
}